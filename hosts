[router]
ISP ansible_ssh_host=192.168.0.150
RTR-L ansible_ssh_host=4.4.4.100
RTR-R ansible_ssh_host=5.5.5.100

[web]
WEB-L ansible_ssh_host=192.168.100.100
WEB-R ansible_ssh_host=172.16.100.100

[all:vars]
ansible_ssh_user = root
ansible_ssh_pass = toor
ansible_python_interpretator = /usr/bin/python3
